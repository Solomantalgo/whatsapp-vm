[supervisord]
nodaemon=true

[program:xvfb]
command=/usr/bin/Xvfb :0 -screen 0 1024x768x16
priority=1
autostart=true
autorestart=true

[program:fluxbox]
command=/usr/bin/fluxbox -display :0
priority=2
autostart=true
autorestart=true

[program:x11vnc]
command=/usr/bin/x11vnc -display :0 -nopw -forever -shared -rfbport 5900
priority=3
autostart=true
autorestart=true

[program:websockify]
command=/usr/bin/websockify --web=/opt/novnc 8080 localhost:5900
priority=4
autostart=true
autorestart=true
